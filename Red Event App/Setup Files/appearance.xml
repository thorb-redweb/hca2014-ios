<appearance>
    <default>
        <barcolor>#02a5e5</barcolor>
        <bartextcolor>#666666</bartextcolor>
        <backcolor>#ffffff</backcolor>
        <backtextcolor>#666666</backtextcolor>
        <altcolor>#ed3030</altcolor>
        <alttextcolor>#666666</alttextcolor>
        <textsize>18.0</textsize>
        <textstyle>normal</textstyle>
        <titlesize>24.0</titlesize>
        <titlestyle>normal</titlestyle>
        <titleshadowoffset>2.0,2.0</titleshadowoffset>
        <titleshadowradius>2.0</titleshadowradius>
        <itemtitlesize>18.0</itemtitlesize>
        <itemtitlestyle>bold</itemtitlestyle>
    </default>
	
    <navigationbar>
        <visible>NO</visible>
    </navigationbar>
	
    <tabbar>
        <backgroundimage>barbackground.png</backgroundimage>
        <textcolor>#000000</textcolor>
        <textsize>12.0</textsize>
        <textstyle>italic</textstyle>
        <textshadowcolor>#FFFFFF</textshadowcolor>
        <textshadowoffset>1.0,1.0</textshadowoffset>
    </tabbar>
	
    <SwipeView>
        <backgroundimage>julebaggrund.jpg</backgroundimage>
        <selectedpagecolor>#b4282e</selectedpagecolor>
        <unselectedpagecolor>#FFFFFF</unselectedpagecolor>
    </SwipeView>
	
    <JuleKalender>
        <backgroundcolor>#00000000</backgroundcolor>
    </JuleKalender>
	
    <Laage>
        <backgroundimage>julebaggrund.jpg</backgroundimage>
        <titlecolor>#b4282e</titlecolor>
        <titlesize>36.0</titlesize>
        <textbackgroundcolor>#FFFFFF</textbackgroundcolor>
        <textcolor>#000000</textcolor>
        <backbuttonimage>btnseeyou.png</backbuttonimage>
    </Laage>
	
    <Nyheder>
        <backgroundcolor>#66151839</backgroundcolor>
        <cellbackgroundcolor>#66151839</cellbackgroundcolor>
    </Nyheder>
	
    <Nyhedsside>
        <backgroundimage>julebaggrund.jpg</backgroundimage>
        <titlecolor>#b4282e</titlecolor>
        <backbuttonimage>btnclose.png</backbuttonimage>
    </Nyhedsside>
	
    <Program>
        <leftarrow>arrow_left.png</leftarrow>
        <rightarrow>arrow_right.png</rightarrow>
    </Program>
	
    <RedWeb>
        <backgroundcolor>#66151839</backgroundcolor>
        <titlecolor>#b4282e</titlecolor>
    </RedWeb>
	
    <RedWebMap>
        <backbuttonimage>btnclose.png</backbuttonimage>
    </RedWebMap>
	
    <RUEvents>
        <backgroundimage>bg_tile.png</backgroundimage>
