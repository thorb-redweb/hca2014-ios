        <camerabuttonimage>icon_camera.png</camerabuttonimage>
        <archivebuttonimage>icon_eye.png</archivebuttonimage>
    </RUOther>
	
    <ContentFolder>
        <backgroundimage>bg_tile.png</backgroundimage>
        <topbuttonbackgroundcolor>#ffffff</topbuttonbackgroundcolor>
        <topbuttontextstyle>normal</topbuttontextstyle>
        <topbuttonbordercolor>#cdcdcd</topbuttonbordercolor>
        <camerabuttonicon>icon_camera.png</camerabuttonicon>
		
        <imagebackgroundcolor>#303030</imagebackgroundcolor>
		
        <itembackgroundcolor>#ffffff</itembackgroundcolor>
        <itembordercolor>#cdcdcd</itembordercolor>
        <approveditemmarkercolor>#60af00</approveditemmarkercolor>
        <unapproveditemmarkercolor>#e1c200</unapproveditemmarkercolor>
        <selecteditembackgroundcolor>#60af00</selecteditembackgroundcolor>
        <selecteditemtextcolor>#ffffff</selecteditemtextcolor>
		
        <approvedtitlecolor>#ffffff</approvedtitlecolor>
		
        <uploadbuttonbackgroundcolor>#60af00</uploadbuttonbackgroundcolor>
        <uploadbuttonbackgroundcolor2>#4b8700</uploadbuttonbackgroundcolor2>
        <uploadbuttonbordercolor>#4b8700</uploadbuttonbordercolor>
        <uploadbuttontextcolor>#ffffff</uploadbuttontextcolor>
		
