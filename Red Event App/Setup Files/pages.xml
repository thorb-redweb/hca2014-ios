<pages>
    <page>
        <type>Global</type>
    </page>
    <page>
        <type>Splash</type>
        <backgroundimage>splash.png</backgroundimage>
    </page>
    <page>
        <name>BikeTracking</name>
        <type>CameraIntent</type>
        <child>Program</child>
        <folder>redApp</folder>
        <frontpage>true</frontpage>
    </page>
    <page>
        <name>AutoSubscriber</name>
        <type>PushMessageAutoSubscriber</type>
        <frontpage>true</frontpage>
        <child>SwipeView</child>
    </page>
    <page>
        <name>TableNavigator</name>
        <type>TableNavigator</type>
        <tabname>Forside</tabname>
        <entry>
            <name>SwipeView</name>
            <backicon>arrow_right.png</backicon>
        </entry>
        <entry>
            <name>Program</name>
            <backicon>arrow_right.png</backicon>
        </entry>
        <entry>
            <name>Nyheder</name>
            <backicon>arrow_right.png</backicon>
        </entry>
        <entry>
            <name>About</name>
            <backicon>arrow_right.png</backicon>
        </entry>
    </page>
    <page>
        <name>SwipeView</name>
        <type>SwipeView</type>
		<section>Nyheder</section>
        <section>PushMessageList</section>
        <section>About</section>
    </page>
    <page>
        <name>Nyheder</name>
        <type>ImageArticleList</type>
        <catid>12</catid>
        <navname>Nyheder</navname>
        <parent>SwipeView</parent>
        <child>Nyhedsside</child>
    </page>
    <page>
        <name>Nyhedsside</name>
        <type>ArticleDetail</type>
        <parent>Nyheder</parent>
        <returnbutton>YES</returnbutton>
    </page>
    <page>
        <name>PushMessageList</name>
        <type>PushMessageList</type>
        <navname>PushMessages</navname>
        <parent>SwipeView</parent>
        <child>PushMessage</child>
    </page>
    <page>
        <name>PushMessage</name>
        <type>PushMessageDetail</type>
        <parent>PushMessageList</parent>
        <returnbutton>YES</returnbutton>
    </page>
    <page>
        <name>About</name>
        <type>StaticArticle</type>
        <articleid>55</articleid>
        <returnbutton>YES</returnbutton>
    </page>
    <page>
        <name>Program</name>
        <type>DailySessionList</type>
        <tabname>Program</tabname>
        <child>Begivenhed</child>
    </page>
    <page>
        <name>Begivenhed</name>
        <type>SessionDetail</type>
        <parent>Program</parent>
        <child>SessionMap</child>
        <returnbutton>YES</returnbutton>
    </page>
    <page>
        <name>SessionMap</name>
        <type>SessionMap</type>
        <parent>Begivenhed</parent>
        <child>Begivenhed</child>
        <returnbutton>YES</returnbutton>
		<zoom>10</zoom>
    </page>
    <page>
        <name>OverviewMap</name>
        <type>OverviewMap</type>
		<longitude>10</longitude>
		<latitude>10</latitude>
		<zoom>10</zoom>
        <tabname>Map</tabname>
        <returnbutton>YES</returnbutton>
    </page>
    <page>
        <name>WebView</name>
        <type>WebView</type>
		<tabname>WebView</tabname>
        <url>http://redweb.dk/</url>
        <returnbutton>YES</returnbutton>
    </page>
    <page>
        <name>RedWeb</name>
        <type>VenueDetail</type>
        <venueid>2</venueid>
        <child>VenueMap</child>
    </page>
    <page>
        <name>VenueMap</name>
        <type>VenueMap</type>
        <parent>RedWeb</parent>
		<longitude>10</longitude>
		<latitude>10</latitude>
		<zoom>10</zoom>
    </page>
